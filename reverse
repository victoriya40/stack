import java.util.*;
public class Main{
    public static void main(String[]args){
        Scanner sc=new Scanner(System.in);
        String s=sc.nextLine();
        Stack<Character>stack=new Stack<>();
        for(char ch:s.toCharArray()){
            stack.push(ch);
        }while(!stack.isEmpty()){
            System.out.println(stack.pop());
        }
    }
}
